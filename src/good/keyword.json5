{
  "repository": {
    "keyword": {
      "patterns": [
        {
          "name": "variable.language.self.snowball",
          "match": "\\b(self)\\b"
        },
        {
          "name": "keyword.control.snowball",
          "match": "\\b(break|continue|else|if|in|for|switch|return|while)\\b"
        },
        {
          "name": "keyword.other.snowball",
          "match": "\\b(external|unsafe|super)\\b"
        },
        {
          "name": "keyword.other.where.snowball",
          "match": "\\b(where)\\b"
        },
        {
          "name": "keyword.other.as.snowball",
          "match": "(\\b(as)\\b)"
        },
        {
          "comment": "Reserved keyword",
          "name": "invalid.other.snowball",
          "match": "\\b(import|private|func|public|operator|class|interface|macro|implements|extends|throw|new)\\b"
        },
      ]
    }
  }
}
