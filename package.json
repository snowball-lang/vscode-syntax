{
    "name": "vscode-snowball-syntax",
    "displayName": "vscode-snowball-syntax",
    "description": "Syntax highlighting for Snowball.",
    "version": "0.0.3",
    "publisher": "MauroBalades",
    "repository": {
        "type": "git",
        "url": "https://github.com/snowball/vscode-syntax.git"
    },
    "bugs": {"url": "https://github.com/snowball/vscode-syntax/issues"},
    "scripts": {"start": "gulp watch"},
    "engines": {"vscode": "^1.20.0"},
    "categories": ["Programming Languages"],
    "v1": true,
    "contributes": {
        "languages": [
            {
                "id": "snowball",
                "aliases": ["snowball"],
                "extensions": [".sn", ".test.sn"],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "snowball",
                "scopeName": "source.snowball",
                "path": "./syntaxes/snowball.tmLanguage.json"
            }
        ]
    },
    "galleryBanner": {
        "color": "#000",
        "theme": "dark"
    },
    "icon": "assets/snowball.png",
    "devDependencies": {
        "gulp": "^3.9.1",
        "gulp-json5-to-json": "^0.2.1",
        "gulp-merge-json": "^1.3.0"
    }
}
